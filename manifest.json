{
    "name":"focusxtn",
    "permissions":[
        "storage"
    ],
    
    "manifest_version": 3,
    "description": "An extension designed to help you focus",
    "version":"1.0.0",

    "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "run_at": "document_idle"
  }
],
    
    "action":{
        "default_popup":"form.html"
    }
}